{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "pop-up-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/popUp/PopUpWebPart.js",
          "manifest": "./src/webparts/popUp/PopUpWebPart.manifest.json"
        }
      ]
    },
    "pivot-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pivot/PivotWebPart.js",
          "manifest": "./src/webparts/pivot/PivotWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "PopUpWebPartStrings": "lib/webparts/popUp/loc/{locale}.js",
    "PivotWebPartStrings": "lib/webparts/pivot/loc/{locale}.js"
  }
}
