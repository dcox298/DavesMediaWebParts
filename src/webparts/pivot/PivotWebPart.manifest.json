    "group": { "default": "Advanced" },
    "title": { "default": "Pivot" },
    "description": { "default": "Pivot description" },
    "officeFabricIconFontName": "Page",
    "properties": {
      "description": "Pivot content",
      "tabs":[{
        "id":0,
        "headerText":"Tab",
        "content":"default content"
      }]
    }
  }]
}
